{
	"name": "icanboogie/bind-prototype",
	"type": "library",
	"description": "Binds icanboogie/prototype to ICanBoogie.",
	"keywords": [ "prototype", "icanboogie" ],
	"homepage": "http://icanboogie.org/",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "http://www.weirdog.com/",
			"role" : "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/ICanBoogie/bind-prototype/issues",
		"source": "https://github.com/ICanBoogie/bind-prototype"
	},

	"minimum-stability": "dev",

	"require": {
		"php": ">=5.5.0",
		"icanboogie/icanboogie": "^3.0.0",
		"icanboogie/prototype": "^2.3.0"
	},

	"autoload": {
		"psr-4": {
			"ICanBoogie\\Binding\\Prototype\\": "lib/"
		}
	},

	"scripts": {
		"post-autoload-dump": "ICanBoogie\\Autoconfig\\Hooks::on_autoload_dump"
	},

	"extra": {
		"icanboogie": {
			"config-constructor": {
				"prototype": "ICanBoogie\\Binding\\Prototype\\Hooks::synthesize_config"
			}
		}
	}
}
